@model IEnumerable<Yackimo.Models.TradeRoutes>

@{
    ViewBag.Title = "ListRoutes";
}

<h2>ListRoutes</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RouteName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RequestorId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RequesteeId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RouteName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RequestorId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RequesteeId)
        </td>
        <td>
            @Html.ActionLink("Products", "RouteProducts", new { id = item.TradeRoutesId })
        </td>
    </tr>
}

</table>
